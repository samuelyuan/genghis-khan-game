{
  "name": "genghis-khan-game",
  "version": "1.0.0",
  "description": "A HTML5 and Javascript remake of the Flash game \"Genghis Khan\"",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "node --watch dist/server.js",
    "build": "node build.js",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "dev:full": "npm run build && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "http": "^0.0.1-security",
    "jsvectormap": "^1.7.0",
    "serve-static": "^2.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jquery": "^3.5.33",
    "@types/serve-static": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.7.3",
    "rimraf": "^6.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelyuan/genghis-khan-game.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samuelyuan/genghis-khan-game/issues"
  },
  "homepage": "https://github.com/samuelyuan/genghis-khan-game#readme",
  "private": true
}
